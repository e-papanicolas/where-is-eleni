{
  "name": "where-is-eleni",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "bun clean && bun fix && astro build",
    "clean": "rimraf dist .astro node_modules",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "bun lint && bun format && bun sync",
    "format": "biome format --write .",
    "lint": "biome check --write --unsafe .",
    "preview": "bun build && bun serve",
    "serve": "bun run ./dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "0.8.1",
    "@astrojs/netlify": "5.4.0",
    "@builder.io/qwik": "^1.5.5",
    "@qwikdev/astro": "^0.5.15",
    "animejs": "^3.2.2",
    "astro": "4.11.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@types/animejs": "^3.1.12"
  }
}
